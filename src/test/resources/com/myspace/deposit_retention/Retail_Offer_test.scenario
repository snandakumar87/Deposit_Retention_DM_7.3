<scenario>
  <name>Retail_Offer_test.scenario</name>
  <maxRuleFirings>100000</maxRuleFirings>
  <globals/>
  <fixtures>
    <fact-data>
      <type>Customer_Profile</type>
      <fieldData>
        <field-data>
          <name>thirdPartyTransfer</name>
          <value>true</value>
          <nature>1</nature>
        </field-data>
      </fieldData>
      <name>customerProfile</name>
      <isModify>false</isModify>
    </fact-data>
    <fact-data>
      <type>EventModel</type>
      <fieldData>
        <field-data>
          <name>EVENT</name>
          <value>ONLINE_LOGIN</value>
          <nature>4</nature>
        </field-data>
      </fieldData>
      <name>eventModel</name>
      <isModify>false</isModify>
    </fact-data>
    <execution-trace/>
    <expect-fact>
      <fieldValues>
        <expect-field>
          <fieldName>OFFER_NAME</fieldName>
          <expected>MRC_OFFER_1</expected>
          <operator>==</operator>
          <nature>1</nature>
        </expect-field>
        <expect-field>
          <fieldName>offerDesc</fieldName>
          <expected>Enjoy $225 on us when you open a new checking account with us today! Make sure to use our Retail Account for the next payment</expected>
          <operator>==</operator>
          <nature>0</nature>
        </expect-field>
      </fieldValues>
      <name>OFFER</name>
      <anonymous>true</anonymous>
    </expect-fact>
  </fixtures>
  <rules/>
  <inclusive>false</inclusive>
  <packageName>com.myspace.deposit_retention</packageName>
  <imports>
    <imports/>
  </imports>
  <ksessions>
    <string>defaultKieSession</string>
  </ksessions>
</scenario>